{
  "name": "ccr-ui-frontend-vue",
  "version": "3.3.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist src-tauri/target node_modules/.vite",
    "clean:all": "rm -rf dist src-tauri/target node_modules",
    "dev:web": "vite --port 5173",
    "build:web": "vite build",
    "build:desktop": "npm run build && tauri build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:check": "cd src-tauri && cargo check",
    "tauri:clippy": "cd src-tauri && cargo clippy",
    "tauri:fmt": "cd src-tauri && cargo fmt",
    "tauri:test": "cd src-tauri && cargo test",
    "check:all": "npm run type-check && npm run lint && npm run tauri:check"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "axios": "^1.7.9",
    "lucide-vue-next": "^0.468.0",
    "pinia": "^2.2.6",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.22",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.3",
    "@types/node": "^22.10.10",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "postcss": "^8.5.1",
    "typescript": "^5.7.3",
    "vite": "^7.1.11",
    "vue-tsc": "^2.2.0"
  }
}
