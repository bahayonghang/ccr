{
  "name": "ccr-ui-frontend-vue",
  "version": "3.18.0",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "node tests/i18n.test.cjs",
    "test:i18n": "node tests/i18n.test.cjs",
    "clean": "rm -rf dist src-tauri/target node_modules/.vite",
    "clean:all": "rm -rf dist src-tauri/target node_modules",
    "dev:web": "vite --port 5173",
    "build:web": "vite build",
    "build:desktop": "npm run build && tauri build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:check": "cd src-tauri && cargo check",
    "tauri:clippy": "cd src-tauri && cargo clippy",
    "tauri:fmt": "cd src-tauri && cargo fmt",
    "tauri:test": "cd src-tauri && cargo test",
    "check:all": "npm run type-check && npm run lint && npm run tauri:check"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@types/marked": "^6.0.0",
    "ansi_up": "^6.0.6",
    "axios": "^1.13.2",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.468.0",
    "marked": "^17.0.1",
    "pinia": "^2.3.1",
    "tailwindcss": "^3.4.19",
    "vue": "^3.5.26",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^22.19.3",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  }
}
